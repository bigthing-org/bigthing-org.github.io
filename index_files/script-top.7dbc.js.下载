!function(a){document.getElementById("viewport");navigator.userAgent.match(/iPhone/i)||navigator.userAgent.match(/iPod/i)?a.getElementById("viewport").setAttribute("content","width=device-width, initial-scale=0.3, user-scalable=yes, minimum-scale=0.4, maximum-scale=1.0"):navigator.userAgent.match(/iPad/i)&&a.getElementById("viewport").setAttribute("content","width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1,0, maximum-scale=1.0")}(document);